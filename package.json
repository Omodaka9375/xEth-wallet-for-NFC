{
  "name": "ethereum-wallet-api-example",
  "version": "0.0.1",
  "description": "a basic example of an API for managing an ethereum wallet",
  "main": "dist",
  "engines": {
    "node": "11.x"
  },
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "debug": "npm run prestart && node --nolazy --inspect-brk=9229 dist"
  },
  "repository": "https://git.heroku.com/xeth.git",
  "author": "Omodaka9375",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "helmet": "^3.12.1",
    "morgan": "^1.8.0",
    "scrypt": "^6.0.3",
    "web3": "^1.0.0-beta.34",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.1",
    "nodemon": "^1.9.2",
    "prettier": "^1.12.1"
  }
}
